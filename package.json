{
	"name": "obsidian-paste-link-with-title",
	"version": "1.0.1",
	"description": "Convert pasted URLs into markdown links with fetched page titles",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run"
	},
	"keywords": [],
	"license": "0-BSD",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/node": "^20.0.0",
		"@vitest/ui": "^4.0.18",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vitest": "^4.0.18",
		"jiti": "2.6.1"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
